{
  "version": 2,
  "functions": {
    "api/upload.js": {
      "maxDuration": 30,
      "memory": 1024
    },
    "api/health.js": {
      "maxDuration": 10,
      "memory": 512
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
